#include <stdio.h>
int PC2L[]={14,17,11,24,1,5,3,28,15,6,21,10,23,19,12,4,26,8,16,7,27,20,13,2};
int PC2R[]={41,52,31,37,47,55,30,40,51,45,33,48,44,49,39,56,34,53,46,42,50,36,29,32};
int main(){
int i;
printf("Left 24 bits from C:\n");
for(i=0;i<24;i++)printf("%d ",PC2L[i]);
printf("\nRight 24 bits from D:\n");
for(i=0;i<24;i++)printf("%d ",PC2R[i]-28);
}
