#include <stdio.h>
#include <string.h>
int p[5][5];char k[]="MONARCHY",a[]="ABCDEFGHIKLMNOPQRSTUVWXYZ";
void f(){
int i,j,m=0;char u[25]={0};
for(i=0;k[i];i++)if(!strchr(u,k[i]))u[m++]=k[i];
for(i=0;a[i];i++)if(!strchr(u,a[i]))u[m++]=a[i];
for(i=0;i<5;i++)for(j=0;j<5;j++)p[i][j]=u[i*5+j];
}
void pos(char c,int* r,int* c2){
int i,j;if(c=='J')c='I';
for(i=0;i<5;i++)for(j=0;j<5;j++)if(p[i][j]==c){*r=i;*c2=j;}
}
int main(){
char c[]="KXJEYUREBEZWEHEWRYTUHEYFSKREHEGOYFIWTTTUOLKSYCAJPOBOTEIZONTXBYBNTGONEYCUZWRGDSONSXBOUYWRHEBAAHYUSEDQ";
int i,r1,c1,r2,c2;f();
for(i=0;c[i];i+=2){
pos(c[i],&r1,&c1);pos(c[i+1],&r2,&c2);
if(r1==r2){putchar(p[r1][(c1+4)%5]);putchar(p[r2][(c2+4)%5]);}
else if(c1==c2){putchar(p[(r1+4)%5][c1]);putchar(p[(r2+4)%5][c2]);}
else{putchar(p[r1][c2]);putchar(p[r2][c1]);}
}
}
