#include <stdio.h>
int inv(int a){for(int i=1;i<26;i++)if(a*i%26==1)return i;return 0;}
int main(){
int P[2][2]={{7,4},{11,15}};
int C[2][2]={{3,2},{5,6}};
int d=(P[0][0]*P[1][1]-P[0][1]*P[1][0]+26)%26;
int di=inv(d);
int Pi[2][2]={{ P[1][1]*di%26,(26-P[0][1])*di%26},
              {(26-P[1][0])*di%26, P[0][0]*di%26}};
int K[2][2];
for(int i=0;i<2;i++)for(int j=0;j<2;j++)
K[i][j]=(C[i][0]*Pi[0][j]+C[i][1]*Pi[1][j])%26;
printf("%d %d\n%d %d\n",K[0][0],K[0][1],K[1][0],K[1][1]);
}
