#include <stdio.h>
#include <string.h>

double eng[]={12.7,9.1,8.2,7.5,7,6.7,6.3,6.1,6,4.3,4,2.8,2.8,2.4,2.4,2.2,2,2,1.9,1.5,1,0.8,0.2,0.2,0.1,0.1};

double score(char *s){
double f[26]={0},sc=0; int n=0;
for(int i=0;s[i];i++) if(s[i]>='A'&&s[i]<='Z'){f[s[i]-'A']++;n++;}
for(int i=0;i<26;i++) sc+=f[i]*eng[i];
return sc;
}

void dec(char *c,char *p,int k){
for(int i=0;c[i];i++) p[i]=(c[i]>='A'&&c[i]<='Z')?'A'+(c[i]-'A'-k+26)%26:c[i];
p[strlen(c)]=0;
}

int main(){
char c[512],p[512]; int top=10;
fgets(c,512,stdin);
double sc[26]; int id[26];
for(int k=0;k<26;k++){dec(c,p,k); sc[k]=score(p); id[k]=k;}
for(int i=0;i<26;i++)for(int j=i+1;j<26;j++)if(sc[j]>sc[i]){
double t=sc[i];sc[i]=sc[j];sc[j]=t; int x=id[i];id[i]=id[j];id[j]=x;}
for(int i=0;i<top;i++){dec(c,p,id[i]); printf("%d: %s",i+1,p);}
}
